{
  "name": "next-template",
  "version": "0.0.2-2",
  "author": "lvbin",
  "description": "a next-template",
  "keywords": [
    "next",
    "template"
  ],
  "homepage": "https://github.com/Simon-Bin/next-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Simon-Bin/next-template.git"
  },
  "bugs": {
    "url": "https://github.com/Simon-Bin/next-template/issues"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "cross-env NODE_ENV=production PORT=3000 node server",
    "start:custom": "node server",
    "export": "next export",
    "lint:script": "eslint . --ext .tsx,.ts",
    "lint:style": "stylelint '**/*.css'",
    "lint": "prettier --write \"**/*.{ts,tsx}\" && npm run lint:script -- --fix && npm run lint:style -- --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "analyze": "cross-env ANALYZE=true yarn build",
    "cz": "git cz",
    "release": "standard-version",
    "github-release": "conventional-github-releaser -p angular"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{css}": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.1",
    "classnames": "^2.2.6",
    "express": "^4.17.1",
    "next": "^9.1.7",
    "next-redux-wrapper": "^4.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@next/bundle-analyzer": "^9.1.7",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.4",
    "@types/react": "^16.9.17",
    "@types/react-redux": "^7.1.5",
    "@types/react-test-renderer": "^16.9.1",
    "@types/redux-mock-store": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "commitizen": "^4.0.3",
    "conventional-github-releaser": "^3.1.3",
    "cross-env": "^6.0.3",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "^3.0.2",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "http-proxy-middleware": "^0.20.0",
    "husky": "^4.0.3",
    "jest": "^24.9.0",
    "less": "^3.11.1",
    "lint-staged": "^9.5.0",
    "nodemon": "^2.0.2",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.12.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "redux-saga-test-plan": "^4.0.0-rc.2",
    "standard-version": "^7.0.1",
    "stylelint": "^12.0.1",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-prettier": "^1.1.2",
    "typescript": "^3.7.4"
  },
  "browserslist": [
    "last 2 version",
    "Firefox ESR",
    "> 1%",
    "ie >= 11"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
